generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Entry{
  id String @id @default(uuid())
  name String
  year String
  type String
  varietal String
  rating String
  consumed Consumed
Date_consumed DateTime? 
  
}

enum Consumed{
  no
  yes
}
 
model User {
 
  name      String 
 password   String @id @default(uuid())
}