generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Entry {
  id            String    @id @default(uuid())
  Date_consumed DateTime?
  consumed      Consumed
  name          String
  rating        String
  type          String
  varietal      String
  year          String
}

model User {
  username     String?
  password String
  email    String  @unique
  id       Int     @id @default(autoincrement())
}

enum Consumed {
  no
  yes
}
